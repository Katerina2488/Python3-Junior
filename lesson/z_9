st = input('Введите строку символов ')
m = int(input('Введите номер начального символа подстроки '))
n = int(input(' Введите номер конечного символа подстроки '))
podst = st[m - 1 : n]
eto_666 = True
for sim in podst:
    if ord(sim)< 54 or ord(sim)>54 :
        eto_666 = False
if eto_666 == True: 
    print('Да, подстрока образует данное число')
else:
    print('Нет, подстрока не образует данное число')

K1=0
S1=0
K2=0
S2=0
for i in range (12):
    n=int(input("Введите целое число "))
    if n%2==0:
        K1=K1+1
        S1=S1+n
    else:
        K2=K2+1
        S2=S2+n
sred1=S1/S2
sred2=S2/K2
print("среднее арифметическое четных чисел равно % 5.2f"%sred1,"Среднее арифметическое нечетных чисел равно % 5.2f"%sred2)
